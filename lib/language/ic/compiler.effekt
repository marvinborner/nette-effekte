module ic

import lib/net/net
import lib/language/ic/term
import lib/language/ic/parser
import lib/language/ic/checker
import lib/language/ic/netter

def compile(): Unit / { Exception[WrongFormat], read[Constructor], NetStream } =
  with source[Constructor] { check!() }
  net!()

def compile!(): Unit / { Exception[WrongFormat], read[Char], NetStream } =
  with source[Constructor] { parse!() }
  compile()